{
  "name": "@rmb/app",
  "version": "1.0.0",
  "author": "Kostiantyn Levytskyi <le.konstantos@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.development.js",
    "start:development": "cross-env PROJECT_ENV=development npm run start",
    "start:staging": "cross-env PROJECT_ENV=staging npm run start",
    "start:production": "cross-env PROJECT_ENV=production npm run start",
    "build": "webpack --config config/webpack.config.production.js",
    "build:development": "cross-env BABEL_ENV=production PROJECT_ENV=development npm run build",
    "build:staging": "cross-env BABEL_ENV=production PROJECT_ENV=staging npm run build",
    "build:production": "cross-env BABEL_ENV=production PROJECT_ENV=production npm run build",
    "clean": "rimraf dist",
    "analyzer": "cross-env NODE_ENV=production BABEL_ENV=production PROJECT_ENV=production webpack --config config/webpack.config.production.js --analyze",
    "eslint": "eslint config src --ext .js --ext .jsx",
    "prettier": "prettier '{config,src}/**/*.{js,jsx}' --write",
    "stylelint": "stylelint 'src/**/*.scss'",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.10.3",
    "@rmb/ui": "^1.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.3.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.0",
    "terser-webpack-plugin": "^2.1.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
